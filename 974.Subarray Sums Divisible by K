        unordered_map<int, int> Dog;  
        int count = 0;
        int sum = 0;
        Dog[0] = 1;

        for (int i = 0; i < nums.size(); i++) {  
            sum += nums[i];
            int remainder = sum % k;

            if (remainder < 0) {   
                remainder += k;
            }

            if (Dog.find(remainder) != Dog.end()) {  
                count += Dog[remainder];
            }

            Dog[remainder]++;
        }

        return count;
    

